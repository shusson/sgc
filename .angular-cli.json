{
    "project": {
        "name": "sgc"
    },
    "apps": [
        {
            "root": "src",
            "assets": [
                "assets",
                ".htaccess",
                "sitemap.xml",
                "robots.txt"
            ],
            "index": "index.html",
            "main": "main.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.app.json",
            "testTsconfig": "tsconfig.spec.json",
            "prefix": "app",
            "styles": [
                "styles.css",
                "../node_modules/tnt.tooltip/_site//build/tnt.tooltip.css",
                "app-theme.scss",
                "../node_modules/@mapd/mapdc/dist/mapdc.css"
            ],
            "scripts": [
                "../node_modules/file-saver/FileSaver.js",
                "../node_modules/@mapd/connector/dist/browser-connector.js",
                "../node_modules/mapbox-gl/dist/mapbox-gl.js",
                "../node_modules/mapbox-gl/dist/mapboxgl-overrides.js"
            ],
            "environmentSource": "environments/environment.ts",
            "environments": {
                "prod": "environments/environment.prod.ts",
                "staging": "environments/environment.staging.ts",
                "demo": "environments/environment.demo.ts",
                "dev": "environments/environment.dev.ts"
            }
        }
    ],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "lint": [
        {
            "project": "src/tsconfig.app.json"
        },
        {
            "project": "src/tsconfig.spec.json"
        }
    ],
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false,
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        }
    }
}
